{
  "name": "@meetup/ux-capture",
  "version": "4.0.0",
  "description": "Browser instrumentation helper that makes it easier to capture UX speed metrics",
  "main": "src/ux-capture.js",
  "directories": {
    "doc": "docs"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/ux-capture.git"
  },
  "keywords": [
    "webperf",
    "perfmatters",
    "web",
    "performance",
    "ux",
    "speed",
    "user-timing"
  ],
  "scripts": {
    "build": "webpack",
    "build:clean": "rm -rf lib/",
    "lint": "eslint . --ext .js,.jsx --fix",
    "test": "yarn test:unit",
    "test:coverage": "yarn test:unit --coverage",
    "test:unit": "NODE_ENV=test jest --config jest.config.json",
    "test:watch": "jest test:unit --watch --verbose",
    "typecheck": "flow",
    "watch": "webpack --watch"
  },
  "author": "Meetup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/ux-capture/issues"
  },
  "homepage": "https://github.com/meetup/ux-capture#readme"
}